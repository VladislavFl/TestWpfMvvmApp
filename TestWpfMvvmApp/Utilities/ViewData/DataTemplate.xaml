<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:TestWpfMvvmApp.ViewModel"
                    xmlns:view="clr-namespace:TestWpfMvvmApp.View">

    <DataTemplate DataType="{x:Type vm:UserViewModel}">
        <view:UserView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:InfoViewModel}">
        <view:InfoView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:GuestViewModel}">
        <view:GuestView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AuthViewModel}">
        <view:AuthView />
    </DataTemplate>

</ResourceDictionary>